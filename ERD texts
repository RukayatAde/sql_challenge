departments
-
dept_no INTEGER
dept_Name VARCHAR

dept_emp
-
emp_no INTEGER FK >- employees.emp_no
dept_no VARCHAR FK >- departments.dept_no

dept_manager
-
dept_no VARCHAR FK >- departments.dept_no
emp_no INTEGER FK >- employees.emp_no

employees
-
emp_no PK INTEGER
emp_title_id INTEGER
birth_date DATE
first_name VARCHAR
last_name VARCHAR
sex INTEGER
hire_date DATE

salaries
-
emp_no INTEGER FK >- employees.emp_no
salary INTEGER

titles
-
title_id INTEGER FK >- employees.emp_title_id
title VARCHAR 
